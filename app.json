{
  "expo": {
    "name": "Aqua 360Â°",
    "slug": "aqua-360",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aqua360",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#52D6E2"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#52D6E2"
      },
      "package": "com.aqua360.app",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#52D6E2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/aqua-360",
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "aqua-360"
      },
      "expoGo": {
        "debuggerHost": "192.168.20.18:8081",
        "packagerHost": "192.168.20.18:8081"
      }
    },
    "developmentClient": {
      "silentLaunch": true
    }
  }
}
